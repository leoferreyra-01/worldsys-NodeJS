{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "Fastify API with all HTTP methods",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "typeorm": "typeorm-ts-node-commonjs",
    "profile:doctor": "clinic doctor -- npm run dev",
    "profile:heap": "clinic heap -- autocannon -c 10 -d 30 http://localhost:3001/users",
    "profile:flame": "clinic flame -- autocannon -c 10 -d 30 http://localhost:3001/users",
    "profile:bubble": "clinic bubbleprof -- autocannon -c 10 -d 30 http://localhost:3001/users",
    "profile:all": "npm run profile:heap && npm run profile:flame && npm run profile:bubble",
    "load:test": "autocannon -c 100 -d 30 http://localhost:3001/users",
    "load:test:all": "autocannon -c 100 -d 30 -m GET,POST,PUT,DELETE http://localhost:3001",
    "perf:test": "node scripts/performance-test.js",
    "perf:test:single": "node scripts/performance-test.js --single"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "fs": "^0.0.1-security",
    "inversify": "^7.5.4",
    "mssql": "^10.0.1",
    "pino-pretty": "^13.0.0",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.8.0",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "fastify",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
